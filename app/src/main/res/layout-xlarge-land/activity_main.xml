<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="180dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/banner"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="140dp"
            app:layout_constraintTop_toTopOf="@+id/banner"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" android:textSize="30sp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/banner"
            app:layout_constraintStart_toEndOf="@+id/logo" android:layout_marginStart="24dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintHorizontal_bias="0.7" app:layout_constraintEnd_toStartOf="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/imageView12"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView12"
            app:layout_constraintStart_toEndOf="@+id/imageView11" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView19" app:layout_constraintTop_toTopOf="@+id/imageView19"
            app:layout_constraintBottom_toBottomOf="@+id/imageView19" android:layout_marginLeft="36dp"
            android:layout_marginRight="36dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView19"
            app:layout_constraintStart_toEndOf="@+id/imageView12" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/button2" app:layout_constraintTop_toTopOf="@+id/button2"
            app:layout_constraintBottom_toBottomOf="@+id/button2" android:layout_marginRight="36dp"/>
    <Button
            android:text="Castomize Dashboard"
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            style="@style/Widget.AppCompat.Button.Borderless" android:background="@drawable/rounded_gradient"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/imageView19" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:textColor="@android:color/white"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/banner"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView20"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toStartOf="@+id/guideline2" android:layout_marginEnd="16dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/logo"
            app:layout_constraintDimensionRatio="16:9"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView21"
            android:scaleType="centerCrop"
            app:layout_constraintTop_toTopOf="@+id/imageView20" android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="@+id/guideline2" app:layout_constraintDimensionRatio="16:9"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView22"
            android:scaleType="centerCrop" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView20" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="@+id/imageView20" app:layout_constraintDimensionRatio="16:9"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView23"
            android:scaleType="centerCrop" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="@+id/guideline2" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView21"
            app:layout_constraintEnd_toEndOf="@+id/imageView21" app:layout_constraintDimensionRatio="16:9"/>
</androidx.constraintlayout.widget.ConstraintLayout>