<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <ImageView
            android:layout_width="683dp"
            android:layout_height="87dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/banner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:background="#212121"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="84dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/banner"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="18sp"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/banner"
            app:layout_constraintStart_toEndOf="@+id/logo" android:layout_marginStart="24dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView24"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintEnd_toStartOf="@+id/imageView25" app:layout_constraintTop_toTopOf="@+id/imageView25"
            app:layout_constraintBottom_toBottomOf="@+id/imageView25"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView25"
            app:layout_constraintStart_toEndOf="@+id/imageView24" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView26" app:layout_constraintTop_toTopOf="@+id/imageView26"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26" android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView26"
            app:layout_constraintStart_toEndOf="@+id/imageView25" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/banner"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView27"
            android:scaleType="centerCrop" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView28"
            app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="16dp" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView28"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView27"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView29"
            app:layout_constraintTop_toTopOf="@+id/imageView29"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView29"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView28"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView30"
            app:layout_constraintTop_toTopOf="@+id/imageView30"
            app:layout_constraintBottom_toBottomOf="@+id/imageView30" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView30"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView29"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/logo"
            app:layout_constraintDimensionRatio="1:1" android:layout_marginEnd="16dp" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>